<?xml version="1.0" encoding="utf-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui">
  <ribbon>
    <tabs>
      <tab id="Bing" label="Bing">
        <group id="BingGroup" label="Bing Group">
          <button id="NewCustomButton"
              imageMso="WebPagePreview"
              size="large"
              label="Search Bing"
              onAction="ButtonClicked"/>
        </group>
      </tab>
      <tab id="Quote" label="Stock Quote">
        <group id="QuoteGroup" label="Quote Group">
          <button id="QuoteButton"
              imageMso="FileWorkflowTasks"
              size="large"
              label="Stock Quote"
              onAction="ButtonClicked"/>
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>